{
  "&": {
    "description": "bitwise AND",
    "inline": "{i0} & {i1}",
    "inputs": [
      "-Integral0",
      "-Integral1"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  "<<": {
    "description": "left shift",
    "inline": "{i0} << {i1}",
    "inputs": [
      "-Integral0",
      "-Integral1"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  ">>": {
    "description": "right shift",
    "inline": "{i0} >> {i1}",
    "inputs": [
      "-Integral0",
      "-Integral1"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  "^": {
    "description": "bitwise XOR",
    "inline": "{i0} ^ {i1}",
    "inputs": [
      "-Integral0",
      "-Integral1"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  "chr": {
    "description": "character of the unicode",
    "inline": "chr({i0})",
    "inputs": [
      "-Integral0"
    ],
    "outputs": [
      "str"
    ]
  },
  "pow3": {
    "description": "raise to the power and modulo",
    "inline": "pow({i0}, {i1}, {i2})",
    "inputs": [
      "-Integral0",
      "-Integral1",
      "-Integral2"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  "|": {
    "description": "bitwise OR",
    "inline": "{i0} | {i1}",
    "inputs": [
      "-Integral0",
      "-Integral1"
    ],
    "outputs": [
      "EGPHighest"
    ]
  },
  "~": {
    "description": "bitwise NOT",
    "inline": "~{i0}",
    "inputs": [
      "-Integral0"
    ],
    "outputs": [
      "-Integral0"
    ]
  }
}
