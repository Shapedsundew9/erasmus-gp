{
    "&": {
        "inline": "{i0} & {i1}",
        "name": "bitwise AND {t0} and {t1}"
    },
    "<<": {
        "inline": "{i0} << {i1}",
        "name": "left shift {t0} by {t1}"
    },
    ">>": {
        "inline": "{i0} >> {i1}",
        "name": "right shift {t0} by {t1}"
    },
    "^": {
        "inline": "{i0} ^ {i1}",
        "name": "bitwise XOR {t0} and {t1}"
    },
    "|": {
        "inline": "{i0} | {i1}",
        "name": "bitwise OR {t0} and {t1}"
    },
    "~": {
        "inline": "~{i0}",
        "name": "bitwise NOT {t0}",
        "num_inputs": 1
    }
}