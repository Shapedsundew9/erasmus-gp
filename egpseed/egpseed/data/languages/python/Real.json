{
    "%": {
        "inline": "{i0} % {i1}",
        "description": "modulo",
        "outputs": [
            "egp_highest"
        ]
    },
    "//": {
        "inline": "{i0} // {i1}",
        "description": "floor divide",
        "outputs": [
            "egp_highest"
        ]
    },
    "<": {
        "inline": "{i0} < {i1}",
        "description": "less than",
        "outputs": [
            "bool"
        ]
    },
    "<=": {
        "inline": "{i0} <= {i1}",
        "description": "less than equal",
        "outputs": [
            "bool"
        ]
    },
    ">": {
        "inline": "{i0} > {i1}",
        "description": "greater than",
        "outputs": [
            "bool"
        ]
    },
    ">=": {
        "inline": "{i0} >= {i1}",
        "description": "greater than equal",
        "outputs": [
            "bool"
        ]
    },
    "divmod": {
        "inline": "divmod({i0}, {i1})",
        "description": "quotient and remainder",
        "outputs": [
            "Pair[egp_highest, egp_highest]"
        ]
    },
    "round1": {
        "inline": "round({i0})",
        "description": "round to nearest integer",
        "num_inputs": 1,
        "outputs": [
            "int"
        ]
    },
    "round2": {
        "inline": "round({i0}, {i1})",
        "inputs": [
            "Real",
            "egp_integral"
        ],
        "description": "round to decimal places",
        "outputs": [
            "egp_wc_0_0"
        ]
    }
}